{
  "name": "pars/pars-master",
  "description": "",
  "license": "BSD-3-Clause",
  "keywords": [
    "pars"
  ],
  "support": {
    "docs": "https://docs.parsphp.org/pars-master/",
    "issues": "https://github.com/pars/pars-master/issues",
    "source": "https://github.com/pars/pars-master",
    "rss": "https://github.com/pars/pars-master/releases.atom",
    "forum": "https://discourse.parsphp.org/"
  },
  "require": {
    "php": "^7.4",
    "ext-intl": "*",
    "ext-json": "*",
    "cache/array-adapter": "^1.1",
    "cache/filesystem-adapter": "^1.0",
    "cache/memcached-adapter": "^1.0",
    "cache/redis-adapter": "^1.0",
    "cocur/slugify": "^4.0",
    "composer/package-versions-deprecated": "^1.10.99",
    "laminas/laminas-cache": "^2.9",
    "laminas/laminas-component-installer": "^2.1.2",
    "laminas/laminas-config-aggregator": "^1.2",
    "laminas/laminas-db": "^2.11",
    "laminas/laminas-diactoros": "^2.3.0",
    "laminas/laminas-eventmanager": "^3.3",
    "laminas/laminas-hydrator": "^3.1",
    "laminas/laminas-i18n": "^2.10",
    "laminas/laminas-log": "^2.12",
    "laminas/laminas-mail": "^2.12",
    "laminas/laminas-serializer": "^2.9",
    "laminas/laminas-servicemanager": "^3.4",
    "laminas/laminas-stdlib": "^3.2.1",
    "laminas/laminas-zendframework-bridge": "^1.0",
    "league/flysystem": "^1.1",
    "league/glide": "^1.6",
    "league/oauth2-client": "^2.6",
    "matthiasmullie/minify": "^1.3",
    "scssphp/scssphp": "^1.3",
    "mezzio/mezzio": "^3.2.1",
    "mezzio/mezzio-authentication": "^1.1",
    "mezzio/mezzio-authentication-session": "^1.0",
    "mezzio/mezzio-csrf": "^1.0",
    "mezzio/mezzio-fastroute": "^3.0.3",
    "mezzio/mezzio-flash": "^1.0",
    "mezzio/mezzio-helpers": "^5.3",
    "mezzio/mezzio-platesrenderer": "^2.2",
    "mezzio/mezzio-session": "^1.3",
    "mezzio/mezzio-session-cache": "^1.4",
    "robertkleinschuster/nice-beans": "^2",
    "twistor/flysystem-http": "^0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.7",
    "squizlabs/php_codesniffer": "^3.5",
    "laminas/laminas-development-mode": "^3.2",
    "mezzio/mezzio-tooling": "^1.3",
    "phpspec/prophecy": "^1.10.3",
    "phpspec/prophecy-phpunit": "^2.0",
    "roave/security-advisories": "dev-master",
    "filp/whoops": "^2.7.1"
  },
  "autoload": {
    "psr-4": {
      "Pars\\Admin\\": "src/Admin/src/",
      "Pars\\Cli\\": "src/Cli/src/",
      "Pars\\Component\\": "src/Component/src/",
      "Pars\\Core\\": "src/Core/src/",
      "Pars\\Frontend\\": "src/Frontend/src/",
      "Pars\\Helper\\": "src/Helper/src/",
      "Pars\\Import\\": "src/Import/src/",
      "Pars\\Model\\": "src/Model/src/",
      "Pars\\Mvc\\": "src/Mvc/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParsTest\\Admin\\": "src/Admin/test/",
      "ParsTest\\Cli\\": "src/Cli/test/",
      "ParsTest\\Component\\": "test/Component/test/",
      "ParsTest\\Core\\": "src/Core/test/",
      "ParsTest\\Frontend\\": "src/Frontend/test/",
      "ParsTest\\Helper\\": "src/Helper/test/",
      "ParsTest\\Import\\": "src/Import/test/",
      "ParsTest\\Model\\": "src/Model/test/",
      "ParsTest\\Mvc\\": "src/Mvc/test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laminas": {
      "module": "Pars\\Master",
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-fastroute"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "development-disable": "php bin/master-development-mode.php disable",
    "development-enable": "php bin/master-development-mode.php enable",
    "development-status": "php bin/master-development-mode.php status",
    "mezzio": "mezzio --ansi",
    "check": [
      "@cs-check",
      "@test"
    ],
    "clear-config-cache": "php bin/clear-config-cache.php",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
